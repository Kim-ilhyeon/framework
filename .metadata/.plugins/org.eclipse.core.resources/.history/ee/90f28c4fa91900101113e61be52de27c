package com.kh.spring.notice.model.dto;

import lombok.Data;
import lombok.NoArgsConstructor;

/**
 * * DTO (Data Transfer Object): 통신/흐름 간에 저장되어야 할 데이터
 */
@NoArgsConstructor
@Data
public class SearchDto {

	private String condition;	// 검색 카테고리
	private String keyword;		// 검색어
	private String orderby;		// 정렬기준
	
}
